    <refentrytitle>latex.biblio.style</refentrytitle>
  </refmeta>
  <refnamediv>
    <refname><parameter>latex.biblio.style</parameter></refname>
    <refpurpose>Default BibTeX style to apply</refpurpose>
  </refnamediv>
  <refsynopsisdiv>
